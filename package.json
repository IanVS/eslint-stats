{
  "name": "eslint-stats",
  "version": "0.1.4",
  "description": "A package of custom formatters that show aggregated stats of eslint errors",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "unit": "jasmine test/**/*.js",
    "test": "npm run lint && istanbul cover --include-all-sources npm run unit",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ganimomer/eslint-stats.git"
  },
  "author": {
    "name": "Omer Ganim",
    "email": "ganimomer@gmail.com",
    "url": "http://github.com/ganimomer"
  },
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "eslint": "^3.13.0",
    "eslint-config-wix-editor": "^0.2.3",
    "istanbul": "^0.3.17",
    "jasmine": "^2.5.2"
  },
  "dependencies": {
    "chalk": "^0.5.1",
    "lodash": "^3.10.0"
  }
}
